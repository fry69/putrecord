{
  "name": "@fry69/putrecord",
  "version": "0.1.0",
  "exports": {
    ".": "./src/main.ts",
    "./lib": "./src/lib.ts"
  },
  "license": "MIT",
  "tasks": {
    "check": "deno fmt --check && deno lint && deno check && deno task test:unit",
    "test": "deno test --allow-net --allow-env --allow-read --allow-write tests/lib.test.ts",
    "test:init": "deno test --allow-net --allow-env --allow-read --allow-write --allow-run tests/init.test.ts",
    "test:unit": "deno test --allow-net --allow-env --allow-read --allow-write --allow-run tests/lib.test.ts tests/init.test.ts",
    "test:e2e": "deno test --allow-net --allow-env --allow-read --allow-write tests/lib.e2e.test.ts",
    "test:all": "deno test --allow-net --allow-env --allow-read --allow-write --allow-run"
  },
  "imports": {
    "@atcute/atproto": "npm:@atcute/atproto@^3.1.4",
    "@atcute/client": "npm:@atcute/client@^4.0.3",
    "@atcute/lexicons": "npm:@atcute/lexicons@^1.1.1",
    "@std/cli": "jsr:@std/cli@^1.0.23",
    "@std/expect": "jsr:@std/expect@^1.0.17"
  }
}
